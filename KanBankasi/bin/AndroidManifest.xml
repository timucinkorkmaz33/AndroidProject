<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kanbankasi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
<uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/kanbankasi"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Mainactivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.kanbankasi.Mainactivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
       
        </activity>
         <activity
            android:name=".acilis"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
       
        </activity>
         <activity
            android:name="com.example.kanbankasi.kayit">
            <category android:name="android.intent.category.DEFAULT" />
        </activity>
         <activity
            android:name="com.example.kanbankasi.sil">
            <category android:name="android.intent.category.DEFAULT" />
        </activity>
        <activity
            android:name="com.example.kanbankasi.Bagisgor">
            <category android:name="android.intent.category.DEFAULT" />
        </activity>
    </application>

</manifest>
